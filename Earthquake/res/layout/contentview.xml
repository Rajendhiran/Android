  			android:layout_height="wrap_content"
  			android:textColor="#EEEEEE"
  			android:shadowColor="#ff0000"
  			android:shadowDx="1.5"
  			android:shadowDy="1"
  			android:shadowRadius="2" />

  		<TextView 
  			android:id="@+id/timevalue"
  			android:textSize="18dip"
  			android:text="17:55:39"
  			android:layout_width="wrap_content"
  			android:layout_height="wrap_content"
  			android:textColor="#385E0F"
  			android:shadowColor="#ff0000"
  			android:shadowDx="1.5"
  			android:shadowDy="1"
  			android:shadowRadius="2" />
  			  			  			  				
  	</TableRow>  	

	<TableRow
  		android:id="@+id/row6"
  		android:layout_width="wrap_content"
  		android:layout_height="wrap_content"
  		android:layout_marginTop="10dip">
  	  		
  		<TextView 
  			android:id="@+id/depth"
  			android:textSize="18dip"
  			android:text="Depth: "  			
  			android:layout_width="wrap_content"
  			android:layout_height="wrap_content"
  			android:textColor="#EEEEEE"
  			android:shadowColor="#ff0000"
  			android:shadowDx="1.5"
  			android:shadowDy="1"
  			android:shadowRadius="2" />

  		<TextView 
  			android:id="@+id/depthvalue"
  			android:textSize="18dip"
  			android:text="33.1"
  			android:layout_width="wrap_content"
  			android:layout_height="wrap_content"
  			android:textColor="#385E0F"
  			android:shadowColor="#ff0000"
  			android:shadowDx="1.5"
  			android:shadowDy="1"
  			android:shadowRadius="2" />
  			  			  			  				
  	</TableRow>  	
  	
  	<TableRow
  		android:id="@+id/row7"
  		android:layout_width="wrap_content"
  		android:layout_height="wrap_content"
  		android:layout_marginTop="10dip">
  	  		
