  			android:id="@+id/lati"
  			android:textSize="18dip"
  			android:text="Latitude: "  			
  			android:layout_width="wrap_content"
  			android:layout_height="wrap_content"
  			android:textColor="#EEEEEE"
  			android:shadowColor="#ff0000"
  			android:shadowDx="1.5"
  			android:shadowDy="1"
  			android:shadowRadius="2" />

  		<TextView 
  			android:id="@+id/lativalue"
  			android:textSize="18dip"
  			android:text="14.0129"
  			android:layout_width="wrap_content"
  			android:layout_height="wrap_content"
  			android:textColor="#385E0F"
  			android:shadowColor="#ff0000"
  			android:shadowDx="1.5"
  			android:shadowDy="1"
  			android:shadowRadius="2" />
  			  			  			  				
  	</TableRow>  	
  	
  	<TableRow
  		android:id="@+id/row8"
  		android:layout_width="wrap_content"
  		android:layout_height="wrap_content"
  		android:layout_marginTop="10dip">
  	  		
  		<TextView 
  			android:id="@+id/longi"
  			android:textSize="18dip"
  			android:text="Longitude: "  			
  			android:layout_width="wrap_content"
  			android:layout_height="wrap_content"
  			android:textColor="#EEEEEE"
  			android:shadowColor="#ff0000"
  			android:shadowDx="1.5"
  			android:shadowDy="1"
  			android:shadowRadius="2" />

  		<TextView 
  			android:id="@+id/longivalue"
  			android:textSize="18dip"
  			android:text="92.9226"
  			android:layout_width="wrap_content"
  			android:layout_height="wrap_content"
  			android:textColor="#385E0F"
  			android:shadowColor="#ff0000"
  			android:shadowDx="1.5"
  			android:shadowDy="1"
  			android:shadowRadius="2" />  			  			  				
  	</TableRow>  	 	
  </TableLayout>
 </LinearLayout>
